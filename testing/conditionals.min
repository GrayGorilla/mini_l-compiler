function main;
beginparams
endparams

beginlocals

  m, n: integer;
  i, j, k: integer;

endlocals

beginbody	## main program

  read m, n;

  i := 1;
  j := 0;
  k := 99999;
  
  ## Conditional
  if (((m <> n))) then
    write i;
  else
    write j;
  endif;

  write k;
  m := 0;

  ## While Loop
  while m <= n beginloop
    write m;
    m := m + 2;
  endloop;

  write k;
  read m;
  n := 0;

  ## Do While Loop
  do beginloop
    write m;
    m := m - 1;
  endloop while m > n;

  write k;
  read n;
  
  ## For Loop
  for i := 0; i < n; i := i + 1 beginloop
    write i;
  endloop;



endbody
